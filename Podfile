platform :ios, '10.0'
inhibit_all_warnings!

target 'ReactorKitApp' do

  use_frameworks!

  # Architecture
  pod 'ReactorKit'

  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'RxOptional'
  pod 'RxViewController'

  # Network
  pod 'Moya'
  pod 'Alamofire'
  pod 'Kingfisher'

  # Data
  pod 'ObjectMapper'

  # UI
  pod 'MJRefresh'

  target 'ReactorKitAppTests' do
    inherit! :search_paths
  end

  target 'ReactorKitAppUITests' do
  end

end
